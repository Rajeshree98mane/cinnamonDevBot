<script type='text/javascript'>
	function initEmbeddedMessaging() {
		try {
			embeddedservice_bootstrap.settings.language = 'en_US'; // For example, enter 'en' or 'en-US'

			embeddedservice_bootstrap.init(
				'00D1s00000012mS',
				'Web_Chat_Bot',
				'https://cinnamon--dev.sandbox.my.site.com/ESWWebChatBot1712317061505',
				{
					scrt2URL: 'https://cinnamon--dev.sandbox.my.salesforce-scrt.com'
				}
			);
		} catch (err) {
			console.error('Error loading Embedded Messaging: ', err);
		}
	};
</script>
<script type='text/javascript' src='https://cinnamon--dev.sandbox.my.site.com/ESWWebChatBot1712317061505/assets/js/bootstrap.min.js' onload='initEmbeddedMessaging()'></script>

