<script type='text/javascript'>
	function initEmbeddedMessaging() {
		try {
			embeddedservice_bootstrap.settings.language = 'en_US'; // For example, enter 'en' or 'en-US'

			embeddedservice_bootstrap.init(
				'00D1s00000012mS',
				'MIAW_Bot_Deployment',
				'https://cinnamon--dev.sandbox.my.site.com/ESWMIAWBotDeployment1712312740922',
				{
					scrt2URL: 'https://cinnamon--dev.sandbox.my.salesforce-scrt.com'
				}
			);
		} catch (err) {
			console.error('Error loading Embedded Messaging: ', err);
		}
	};
</script>
<script type='text/javascript' src='https://cinnamon--dev.sandbox.my.site.com/ESWMIAWBotDeployment1712312740922/assets/js/bootstrap.min.js' onload='initEmbeddedMessaging()'></script>

